			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"// Convert XML response to JSON",
							"var jsonObject = xml2Json(responseBody);",
							"",
							"// Convert the JSON object to a string for checking if it contains specific keys",
							"var jsonString = JSON.stringify(jsonObject);",
							"",
							"pm.test(\"Body contains 'Envelope'\", function () {",
							"    pm.expect(jsonString).to.include(\"Envelope\");",
							"});",
							"",
							"pm.test(\"Body contains 'Body'\", function () {",
							"    pm.expect(jsonString).to.include(\"Body\");",
							"});",
							"",
							"pm.test(\"Body contains 'IsValidISBN13Response'\", function () {",
							"    pm.expect(jsonString).to.include(\"IsValidISBN13Response\");",
							"});",
